    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>2dae5eec-d1ab-488f-a438-89e0a33d6031</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.5" />
